@model EmployeeList

<div class="row justify-content-center">
    <div class="col-xl-6 col-lg-8 col-md-8 col-sm-10">
        <table class="table">
            <thead>
                <tr class="table-primary">
                    <th scope="col">Id</th>
                    <th scope="col">First Name</th>
                    <th scope="col">Last Name</th>
                    <th scope="col"></th>
                </tr>
            </thead>
            <tbody>
                @if(Model.Employees != null )
                {
                    @foreach (var t in Model.Employees)
                    {
                        <tr>
                            <th class="todos-column" scope="row">@t.Id</th>
                            <td class="todos-column">@t.FName</td>
                            <td class="todos-column">@t.LName</td>
                            <td class="buttons-column">
                                <input type="submit" class="btn btn-danger" value="Delete" onclick="" />
                                <input type="submit" class="btn btn-primary" value="Update" onclick="" />
                            </td>
                        </tr>
                    }
                }
                
                
            </tbody>
        </table>
    </div>
</div>